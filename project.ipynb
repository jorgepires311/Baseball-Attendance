    "atbats_pd = pd.read_csv(atbats_file)\n",
    "player_name_pd = pd.read_csv(player_name_file)\n",
    "\n",
    "games_atbats_merge_pd = pd.merge(games_pd, atbats_pd, how=\"left\", on=[\"g_id\"])\n",
    "\n",
    "games_atbats_merge_pd.columns = ['Attendance',\n",
